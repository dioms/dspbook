<h1>Members</h1>
<table class="table">
  <thead>
    <tr>
      <th>Picture</th>
      <th>Name</th>
      <th>Resume</th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= image_tag user.profpic.url(:thumb) %></td>
        <td><%= user.name %> </td>
        <td><%= link_to 'Resume', user.resume.url %> </td>
    <% end %>
  </tbody>
</table> <br />
